{
  "manifest_version": 3,
  "name": "Basic Example Extension",
  "version": "1.0.0",
  "description": "A basic example of a Chrome extension",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"]
}
